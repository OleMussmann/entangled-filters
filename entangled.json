{
  "entangled": {
    "annotate": "Standard",
    "database": ".entangled/db.sqlite",
    "languages": [
      {
        "comment": "#",
        "identifiers": [
          "awk"
        ],
        "name": "Awk"
      },
      {
        "comment": {
          "end": "*/",
          "start": "/*"
        },
        "identifiers": [
          "c"
        ],
        "name": "C"
      },
      {
        "comment": "//",
        "identifiers": [
          "cpp",
          "c++"
        ],
        "name": "C++"
      },
      {
        "comment": ";",
        "identifiers": [
          "clojure"
