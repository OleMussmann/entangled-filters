    "lineDirectives": [
      {
        "format": "#LINE {linenumber} \"{filename}\"",
        "name": "C"
      },
      {
        "format": "#LINE {linenumber} \"{filename}\"",
        "name": "C++"
      },
      {
        "format": "{{-# LINE {linenumber} \"{filename}\" #-}}",
        "name": "Haskell"
      }
    ],
    "useLineDirectives": false,
    "version": "1.0.0",
    "watchList": [
      "lit/*.md"
    ]
  },
  "jupyter": [
    {
      "kernel": "python3",
      "language": "Python"
    }
  ]
